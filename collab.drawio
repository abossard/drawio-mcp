                <mxCell id="sec4-mempressure" value="ðŸ§  MemoryPressure&amp;#xa;&amp;#xa;Signal: memory.available&amp;#xa;Hard default: &lt; 100Mi&amp;#xa;Taint: node.kubernetes.io/&amp;#xa;  memory-pressure:NoSchedule" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#ffcdd2;strokeColor=#c62828;fontSize=10;shadow=1;align=left;spacingLeft=8;" parent="1" vertex="1">
