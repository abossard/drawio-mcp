                <mxCell id="sec3-box" value="ðŸŽ¯ 3. Scheduler Placement Strategies" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#e8f5e9;strokeColor=#2e7d32;verticalAlign=top;fontSize=15;fontStyle=1;fontColor=#1b5e20;dashed=1;dashPattern=5 5;strokeWidth=2;" parent="1" vertex="1">
