                <mxCell id="sec3-scoring-hdr" value="ðŸ“Š NodeResourcesFit Plugin â€” The #1 Bin Packing Lever" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#263238;strokeColor=#37474f;fontColor=#ffffff;fontSize=12;fontStyle=1;" parent="1" vertex="1">
